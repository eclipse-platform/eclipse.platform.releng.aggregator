          <predicate
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.ecf"/>
        </workingSet>
      </setupTask>
      <stream
          name="master"
          label="Master"/>
      <description>The Platform's ECF dependencies</description>
    </project>
    <project
